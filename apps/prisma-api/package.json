{
    "name": "@pyromancy/prisma-api",
    "version": "0.2.1",
    "description": "Prisma Example",
    "private": false,
    "type": "module",
    "scripts": {
        "gui": "pnpm exec prisma studio",
        "\n========== BUILD >> ==========": "",
        "build": " pnpm build:ts",
        "build:ts": "tsc --build",
        "\n========== HELP >> ==========": "",
        "help": "pnpm run test:build:cli --help",
        "\n========== DEV >> ==========": "",
        "dev": "tsc  --build --watch",
        "\n========== TEST >> ==========": "",
        "test": "pnpm exec ts-node --esm ./src/index.ts",
        "\n========== PUBLISH >> ==========": "",
        "build:deploy": "pnpm clean && pnpm build:esbuild && pnpm build:cp &&  pnpm push",
        "\n========== CLEAN >> ==========": "",
        "clean": "pnpm clean:ts && pnpm clean:build",
        "clean:build": "rm -R -f dist && rm -R -f types",
        "clean:ts": "tsc --build --clean",
        "\n========== CODE STYLE >> ==========": "",
        "lint": "cross-var eslint \"{$npm_package_config_eslint_path}.{$npm_package_config_eslint_extensions}\" --ignore-path .gitignore -c $npm_package_config_eslint_config",
        "prettier": "cross-var prettier {$npm_package_config_prettier_path}.{$npm_package_config_prettier_extensions} --ignore-path ./.gitignore --write",
        "\n========== CHECK >> ==========": "",
        "check": "pnpm build:ts && pnpm lint",
        "\n========== FIX >> ==========": "",
        "fix": "pnpm prettier"
    },
    "homepage": "https://github.com/gbtunney/snailicide-monorepo#readme",
    "bugs": {
        "url": "https://github.com/gbtunney/snailicide-monorepo/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gbtunney/snailicide-monorepo.git"
    },
    "license": "MIT",
    "author": {
        "name": "Gillian Tunney",
        "email": "gbtunney@mac.com"
    },
    "devDependencies": {
        "@prisma/client": "^5.0.0",
        "@snailicide/build-config": "^0.2.5",
        "@snailicide/g-library": "^0.2.1",
        "@types/node": "^20.4.5",
        "eslint": "^8.45.0",
        "prettier": "^3.0.0",
        "prettier-plugin-prisma": "^5.0.0",
        "prettier-plugin-xml": "^0.1.0",
        "prisma": "^5.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "zod": "^3.21.4"
    },
    "config": {
        "document": {
            "id": "10xi91YyCk3asES5Tg4RjWIHVONggtxD_LcxPpANbiQc"
        },
        "eslint": {
            "extensions": "ts,js,cjs,mjs",
            "path": ".es*,*,**/*",
            "config": ".eslintrc.cjs"
        },
        "prettier": {
            "extensions": "prisma,xml,html,ts,js,cjs,mjs,cts,json,yaml,yml,graphql,md",
            "path": "*,**/*"
        }
    }
}
